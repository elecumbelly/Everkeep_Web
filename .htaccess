<IfModule mod_authz_core.c>
  <FilesMatch "^\.env">
    Require all denied
  </FilesMatch>
  <FilesMatch "^.*\.sql$">
    Require all denied
  </FilesMatch>
</IfModule>

<IfModule !mod_authz_core.c>
  <FilesMatch "^\.env">
    Order allow,deny
    Deny from all
  </FilesMatch>
  <FilesMatch "^.*\.sql$">
    Order allow,deny
    Deny from all
  </FilesMatch>
</IfModule>
